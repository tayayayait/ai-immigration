{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/|_next/|static/|favicon.ico).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/*.js": {
            "maxDuration": 10
        }
    }
}